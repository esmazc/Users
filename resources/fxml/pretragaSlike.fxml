<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="5.0" vgap="5.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox spacing="5.0" GridPane.hgrow="NEVER" GridPane.vgrow="NEVER">
         <children>
            <TextField fx:id="fldPretraga" prefHeight="26.0" prefWidth="408.0" />
            <Button mnemonicParsing="false" onAction="#searchAction" text="%search" />
            <Button mnemonicParsing="false" onAction="#okAction" text="%ok" />
            <Button mnemonicParsing="false" onAction="#cancelAction" text="%canc" />
         </children></HBox>
      <ScrollPane fx:id="scrollPane" hbarPolicy="NEVER" prefHeight="500.0" prefWidth="600.0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
        <content>
          <TilePane fx:id="tilePane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" />
        </content>
      </ScrollPane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
   </padding>
</GridPane>
